<%= form_with model:@kitten do |form|%>
  <%= form.label :name%>
  <%= form.text_field :name%><br /><br />
  <%= form.label :age%>
  <%= form.text_field :age%><br /><br />
  <%= form.label :cuteness%>
  <%= form.text_field :cuteness%><br /><br />
  <%= form.label :softness%>
  <%= form.text_field :softness%><br /><br />
  <%= form.submit%>
<% end %>